    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.16.0",
    "bootstrap": "^3.3.7",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "css-loader": "^0.27.3",
    "enzyme": "^2.8.0",
    "eslint": "2.x",
    "eslint-plugin-import": "^2.2.0",
