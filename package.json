    "socket.io-client": "^1.5.0",
    "style-loader": "^0.16.0",
    "url-loader": "^0.5.8",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
